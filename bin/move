#!/usr/bin/env ruby
require_relative '../lib/move.rb'

puts "Welcome to Tic Tac Toe!
Where would you like to go?"

user_input = gets.strip
converted_input = gets.strip
move = converted_input

if move == 2 or move == 1 or move == 3 or move == 4 or move == 5 or move == 6 or move == 7 or move == 8 or move == 9
  puts display_board(move)
  else
    puts "Invalid"
  end
