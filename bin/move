#!/usr/bin/env ruby

require_relative '../lib/move.rb'

puts "Welcome to Tic Tac Toe!
Where would you like to go?"
number = gets.strip
puts "X or O?"
player = gets.strip
  if player = "X"
  puts display_board(number)
  puts "X"
  else puts nil
  end
  if player = "O"
  puts display_board(number)
  puts "O"
  else puts nil
  end
